{
  "name": "stock-news-bot",
  "version": "1.0.0",
  "description": "Crypto and stock news based Discord bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "increment": "npm version patch",
    "start": "ts-node ./bin/app",
    "start:dev": "nodemon --inspect",
    "tslint": "tslint --project tsconfig.json --config tslint.json",
    "tslint-fix": "tslint --project tsconfig.json --config tslint.json --fix",
    "version": "auto-changelog -p --hide-credit && git add CHANGELOG.md && npm run increment"
  },
  "keywords": [
    "stock",
    "news",
    "discord",
    "bot"
  ],
  "author": "Jake Lindsey",
  "license": "ISC",
  "dependencies": {
    "@typeit/discord": "^4.0.9",
    "@types/axios": "^0.14.0",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.28",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/semver": "^7.3.5",
    "auto-changelog": "^2.2.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.2"
  },
  "engines": {
    "node": "~12.18.4",
    "npm": "~6.14.6"
  }
}
